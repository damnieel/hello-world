public enum EnumOperation {
	AND("and"),
	OR("or"),
	IN("in"),
	NOT_IN("not in"),
	EQUAL("="),
	NOT_EQUAL("!="),
	LEFT_JOIN("left join"),
	RIGHT_JOIN("right join"),
	JOIN("join");

	public String key;

	/**
	 * 
	 * @param key    key
	 */
	EnumOperation(String key){
		this.key = key;
	}
}