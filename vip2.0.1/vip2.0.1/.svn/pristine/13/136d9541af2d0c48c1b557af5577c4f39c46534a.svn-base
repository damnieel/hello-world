package com.moon.vip.infra.vo;

import java.io.Serializable;

/**
 * 
 * @ClassName: LoginInfoVO 
 * @Description: 验证登录信息
 * @author 大枫
 * @date 2016年1月8日 下午12:29:26 
 *
 */
public class LoginInfoVO implements Serializable{

	private static final long serialVersionUID = -138951510168877716L;
	/**
	 * 员工工号
	 */
	private String employeeCode;
	/**
	 * 员工登陆密码
	 */
	private String password;
	/**
	 * 员工验证码
	 */
	private String code;
	@Override
	public String toString() {
		return "LoginInfoVO [employeeCode=" + employeeCode + ", password=" + password + ", code=" + code + "]";
	}
	public String getEmployeeCode() {
		return employeeCode;
	}
	public void setEmployeeCode(String employeeCode) {
		this.employeeCode = employeeCode;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}	

}
