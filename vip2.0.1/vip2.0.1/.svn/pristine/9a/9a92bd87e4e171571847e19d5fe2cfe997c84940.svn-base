package com.moon.vip.infra.vo.attendance;

import java.util.Date;

/**
 * 
* @ClassName: AttendRecordVO 
* @Description: TODO(考勤单条记录模型) 
* @author  陈海  - （怡宝 ）
* @date 2015年12月24日 下午4:29:28 
*
 */
public class AttendErrorRecordVO {
	
    private Integer professionId;
    private Integer classId;
    private Integer studentId;
    private Date inspectDate;
    private Integer inspectStatus;
    private String reason;
    
	public Integer getProfessionId() {
		return professionId;
	}
	public void setProfessionId(Integer professionId) {
		this.professionId = professionId;
	}
	public Integer getClassId() {
		return classId;
	}
	public void setClassId(Integer classId) {
		this.classId = classId;
	}
	public Integer getStudentId() {
		return studentId;
	}
	public void setStudentId(Integer studentId) {
		this.studentId = studentId;
	}
	public Date getInspectDate() {
		return inspectDate;
	}
	public void setInspectDate(Date inspectDate) {
		this.inspectDate = inspectDate;
	}
	public Integer getInspectStatus() {
		return inspectStatus;
	}
	public void setInspectStatus(Integer inspectStatus) {
		this.inspectStatus = inspectStatus;
	}
	public String getReason() {
		return reason;
	}
	public void setReason(String reason) {
		this.reason = reason;
	}
	@Override
	public String toString() {
		return "AttendErrorRecordVO [professionId=" + professionId + ", classId=" + classId + ", studentId="
				+ studentId + ", inspectDate=" + inspectDate + ", inspectStatus=" + inspectStatus + ", reason="
				+ reason + "]";
	}

}
