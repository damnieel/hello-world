<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.moon.vip.application.sys.ActionTypeMapper" >
  <select id="selectAllAct" resultType="com.moon.vip.model.sys.ActionType">
  SELECT act.ID, act.type, act.type_name as typeName FROM `vip_action_type` act WHERE is_valid='Y' order by act.ID;
  </select>
</mapper>