package com.moon.vip.infra.vo.student;

import java.io.Serializable;

public class StudentClassVO implements Serializable{
	
	 /** 
	* @Fields serialVersionUID : TODO( ) 
	*/ 
	private static final long serialVersionUID = -7222771721979478575L;
	private int scId;
	private String nickName;
	private String professionName;
	private double score;
	public String getNickName() {
		return nickName;
	}
	public void setNickName(String nickName) {
		this.nickName = nickName;
	}
	public String getProfessionName() {
		return professionName;
	}
	public void setProfessionName(String professionName) {
		this.professionName = professionName;
	}
	public double getScore() {
		return score;
	}
	public void setScore(double score) {
		this.score = score;
	}
	@Override
	public String toString() {
		return "StudentClassVO [scId=" + scId + ", nickName=" + nickName + ", professionName=" + professionName
				+ ", score=" + score + "]";
	}
	public int getScId() {
		return scId;
	}
	public void setScId(int scId) {
		this.scId = scId;
	}
	
	 
}
